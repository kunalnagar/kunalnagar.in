{
  "name": "kunalnagar.in",
  "version": "2.0.0",
  "description": "Personal website",
  "homepage": "https://www.kunalnagar.in",
  "repository": {
    "type": "git",
    "url": "git@github.com:kunalnagar/kunalnagar.in.git"
  },
  "author": "Kunal Nagar <knlnagar@gmail.com>",
  "scripts": {
    "build": "npm run build:jekyll",
    "build:jekyll": "JEKYLL_ENV=production bundle exec jekyll build",
    "dev": "bundle exec jekyll clean && npm run dev:jekyll",
    "dev:jekyll": "sleep 5 && bundle exec jekyll serve --watch --host=0.0.0.0",
    "logs": "docker-compose logs -f",
    "pretty-quick": "pretty-quick",
    "release": "semantic-release",
    "start": "docker-compose up -d --build",
    "stop": "docker-compose down -v"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*": [
      "pretty-quick"
    ],
    "package.json": [
      "sort-package-json"
    ]
  },
  "devDependencies": {
    "@babel/core": "7.16.7",
    "@babel/preset-env": "7.16.7",
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/git": "9.0.1",
    "babel-loader": "8.2.3",
    "conventional-changelog-conventionalcommits": "4.6.3",
    "husky": "4.3.8",
    "lint-staged": "11.2.6",
    "prettier": "2.5.1",
    "pretty-quick": "3.1.3",
    "semantic-release": "17.4.7",
    "sort-package-json": "1.53.1"
  }
}
